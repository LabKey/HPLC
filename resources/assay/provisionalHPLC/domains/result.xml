<ap:domain xmlns:exp="http://cpas.fhcrc.org/exp/xml" xmlns:ap="http://labkey.org/study/assay/xml">
    <exp:Description>HPLC Assay Results</exp:Description>
    <exp:PropertyDescriptor>
        <exp:Name>Name</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Sample</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Related Sample</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Dilution</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#int</exp:RangeURI>
        <exp:Label>Dilution Factor</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Diluent</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>TestType</exp:Name>
        <exp:Description>'SMP' for samples, 'STD' for standards</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Type</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>DataFile</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <!-- http://cpas.fhcrc.org/exp/xml#fileLink -->
    </exp:PropertyDescriptor>
</ap:domain>
